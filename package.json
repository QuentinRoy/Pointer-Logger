{
  "name": "track-recorder",
  "version": "1.3.0",
  "description": "Application to record and export drags.",
  "main": "build",
  "scripts": {
    "start": "webpack-dev-server --define process.env.NODE_ENV=\"'development'\" --config ./build-config/webpack.dev.js --progress",
    "build": "NODE_ENV=production webpack --config ./build-config/webpack.prod.js --progress",
    "serve": "serve ./build",
    "test": "NODE_ENV=test ava",
    "lint": "eslint src",
    "release": "standard-version",
    "git-hook": "npm run lint && npm run test",
    "postrewrite": "npm run git-hook",
    "precommit": "npm run git-hook",
    "commitmsg": "validate-commit-msg"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/QuentinRoy/Track-Recorder.git"
  },
  "keywords": [
    "drag",
    "export",
    "record",
    "gesture"
  ],
  "author": "Quentin Roy <quentin@quentinroy.fr>",
  "license": "MIT",
  "private": true,
  "devDependencies": {
    "ava": "^0.21.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babili-webpack-plugin": "^0.1.2",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.29.0",
    "husky": "^0.14.3",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.5",
    "pug": "^2.0.0-rc.2",
    "sass-lint": "^1.10.2",
    "sass-loader": "^6.0.6",
    "serve": "^6.0.2",
    "standard-version": "^4.2.0",
    "url-loader": "^0.5.9",
    "validate-commit-msg": "^2.12.2",
    "vue-loader": "^12.2.2",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.3.0",
    "webpack-dev-server": "^2.5.1",
    "webpack-merge": "^4.1.0"
  },
  "dependencies": {
    "downloadjs": "^1.4.7",
    "raf-throttle": "^2.0.3",
    "rxjs": "^5.4.2",
    "vue": "^2.4.2"
  },
  "config": {
    "validate-commit-msg": {
      "types": "conventional-commit-types"
    }
  },
  "browserslist": ">5%",
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  }
}
